@model List<OfertasModel>

<div class="card card-default">
    <div class="card-header">
        <h2>Ofertas</h2>
    </div>

    <div class="card-body">

        <table class="table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Nombre</th>
                    <th>Cantidad</th>
                    <th>Horario</th>
                    <th>Salario</th>
                </tr>
            </thead>

            <tbody>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Nombre</td>
                        <td>@item.Cantidad</td>
                        <td>@item.Horario</td>
                        <td>@item.Salario</td>
                    </tr>
                }

            </tbody>
        </table>

    </div>
</div>